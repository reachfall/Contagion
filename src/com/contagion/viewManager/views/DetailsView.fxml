<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
         prefWidth="1000.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.contagion.viewManager.controllers.DetailsView">
    <Tab text="Clients">
        <TableView fx:id="clientTableView">
            <columns>
                <TableColumn fx:id="clientNameCol" prefWidth="100" style="-fx-alignment: center" text="Name"/>
                <TableColumn fx:id="clientSurnameCol" prefWidth="100" style="-fx-alignment: center" text="Surname"/>
                <TableColumn fx:id="clientIdCol" prefWidth="250" style="-fx-alignment: center" text="Id"/>
                <TableColumn fx:id="clientNextShopCol" prefWidth="150" style="-fx-alignment: center" text="Next Shop"/>
                <TableColumn fx:id="clientCartOccupancyCol" prefWidth="100" style="-fx-alignment: center"
                             text="Cart occupancy"/>
                <TableColumn fx:id="clientIsSickCol" prefWidth="100" style="-fx-alignment: center" text="Is sick"/>
                <TableColumn fx:id="clientIsMaskedCol" prefWidth="100" style="-fx-alignment: center" text="Is masked"/>
                <TableColumn fx:id="clientIsVaccinatedCol" prefWidth="100" style="-fx-alignment: center"
                             text="Is vaccinated"/>
                <TableColumn fx:id="removeClient" prefWidth="100" style="-fx-alignment: center" text="Delete client"/>
            </columns>
        </TableView>
    </Tab>
    <Tab text="Suppliers">
        <TableView fx:id="supplierTableView">
            <columns>
                <TableColumn fx:id="supplierNameCol" prefWidth="100" style="-fx-alignment: center" text="Name"/>
                <TableColumn fx:id="supplierSurnameCol" prefWidth="100" style="-fx-alignment: center" text="Surname"/>
                <TableColumn fx:id="supplierCompanyName" prefWidth="100" style="-fx-alignment: center"
                             text="Company name"/>
                <TableColumn fx:id="supplierIdCol" prefWidth="250" style="-fx-alignment: center" text="Id"/>
                <TableColumn fx:id="supplierTankOccupancyCol" prefWidth="100" style="-fx-alignment: center"
                             text="Fuel level"/>
                <TableColumn fx:id="supplierTrunkOccupancyCol" prefWidth="100" style="-fx-alignment: center"
                             text="Trunk occupancy"/>
                <TableColumn fx:id="supplierIsSickCol" prefWidth="100" style="-fx-alignment: center" text="Is sick"/>
                <TableColumn fx:id="supplierIsMaskedCol" prefWidth="100" style="-fx-alignment: center"
                             text="Is masked"/>
                <TableColumn fx:id="supplierIsVaccinatedCol" prefWidth="100" style="-fx-alignment: center"
                             text="Is vaccinated"/>
                <TableColumn fx:id="removeSupplier" prefWidth="100" style="-fx-alignment: center"
                             text="Delete supplier"/>
            </columns>
        </TableView>
    </Tab>
    <Tab text="Retail shops">
        <TableView fx:id="retailShopTableView">
            <columns>
                <TableColumn fx:id="retailShopNameCol" prefWidth="100" text="Name"/>
                <TableColumn fx:id="retailShopIdCol" prefWidth="250" style="-fx-alignment: center" text="Id"/>
                <TableColumn fx:id="retailShopSupplyOccupancyCol" prefWidth="100" text="Supply"/>
            </columns>
        </TableView>
    </Tab>
    <Tab text="Wholesale">
        <TableView fx:id="wholesaleTableView">
            <columns>
                <TableColumn fx:id="wholesaleNameCol" prefWidth="100" text="Name"/>
                <TableColumn fx:id="wholesaleIdCol" prefWidth="250" style="-fx-alignment: center" text="Id"/>
                <TableColumn fx:id="wholesaleSupplyOccupancyCol" prefWidth="100" text="Supply"/>
            </columns>
        </TableView>
    </Tab>
    <Tab text="Products">
        <TableView fx:id="productTableView">
            <columns>
                <TableColumn fx:id="productNameCol" prefWidth="100" text="Name"/>
                <TableColumn fx:id="productCompanyNameCol" prefWidth="100" text="Company name"/>
                <TableColumn fx:id="productIdCol" prefWidth="250" style="-fx-alignment: center" text="Id"/>
                <TableColumn fx:id="productBBDateCol" prefWidth="100" text="BB date"/>
                <TableColumn fx:id="productFromCol" prefWidth="100" text="From"/>
                <TableColumn fx:id="productToCol" prefWidth="100" text="To"/>
                <TableColumn fx:id="productDeliveredByCol" prefWidth="100" text="Delivered by"/>
                <TableColumn fx:id="productBoughtByCol" prefWidth="100" text="Bought by"/>
                <TableColumn fx:id="productExistsCol" prefWidth="100" text="Exists"/>
                <TableColumn fx:id="productDisposedCol" prefWidth="100" text="Disposed"/>
            </columns>
        </TableView>
    </Tab>
</TabPane>
